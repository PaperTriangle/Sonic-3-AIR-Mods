function bool Standalone.onWriteToSpriteTable(s16 px, s16 py, u16 renderQueue)
{
	u8 gameMode = global.game_mode & 0x7f

	if (gameMode == 0x0c || gameMode == 0x08) && (dropdashdust >= 2)
	{
		if (objA0.update_address == 0x018b3e && objA0.state == 0x00) && (maniadustframe > 0) && !(objA0.render_flags & render_flag.VISIBLE)
		{
				maniadustframe = 0
				maniadustanimtimer = 0
		}
	}
	
	return base.Standalone.onWriteToSpriteTable(px, py, renderQueue)
}