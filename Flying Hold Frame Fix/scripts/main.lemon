function u64 stringformat(u64 format, u64 arg1)
{
	if (char.state == char.state.SONIC_HANGON && arg1 >= 0x90 && arg1 <= 0x92)
	{	
		u8 ani = ((level.framecounter >> 5) % 4)
		
		if (ani == 0)
			arg1 = 0x90
		if (ani == 1 || ani == 3)
			arg1 = 0x91
		if (ani == 2)
			arg1 = 0x92
	}
	
	return base.stringformat(format, arg1)
}